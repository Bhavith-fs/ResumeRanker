{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/spa",
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/spa/$1"
    }
  ]
}
